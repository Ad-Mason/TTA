
var temperature
var condition
var goToWorkhttps://github.com/Ad-Mason/TTA

temperature = prompt("what is the temperature?")

condition = prompt("what is the weather condition?")


function Decide(temperature, condition) {
 if(temperature > 10)  {
 switch (condition) {
  case "sunny" :
    goToWork = true
  break;
  case "cloudy":
    goToWork = true
  break;
  case "raining" :
    goToWork = false
  break;
  case "snowing" :
    goToWork = false
  break;
  case "thunder" :
    goToWork = false
  break;   }
 }
}

if (goToWork) {
  console.log("go to work")
}
